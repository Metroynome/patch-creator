ENTRY(_start);

SECTIONS {

        .text 0x01DFF000: {



                _ftext = . ;
                *(.text)

                KEEP(*(.config))

                *(.text.*)
                *(.gnu.linkonce.t*)
        }


        PROVIDE(_etext = .);
        PROVIDE(etext = .);

        .reginfo : { *(.reginfo) }


        .rodata ALIGN(16): {
                *(.rodata)
                *(.rodata.*)
                *(.gnu.linkonce.r*)
        }

        .data ALIGN(16): {
                _fdata = . ;
                *(.data)
                *(.data.*)
                *(.gnu.linkonce.d*)
                SORT(CONSTRUCTORS)
        }

        .sdata ALIGN(16): {
                *(.sdata)
                *(.sdata.*)
                *(.gnu.linkonce.s*)
        }

        _edata = .;
        PROVIDE(edata = .);


        .sbss ALIGN(16) : {
                _fbss = . ;
                *(.sbss)
                *(.sbss.*)
                *(.gnu.linkonce.sb*)
                *(.scommon)
        }

        .bss ALIGN(16) : {
                *(.bss)
                *(.bss.*)
                *(.gnu.linkonce.b*)
                *(COMMON)
        }
        _end_bss = .;

        _end = . ;
        PROVIDE(end = .);
}
